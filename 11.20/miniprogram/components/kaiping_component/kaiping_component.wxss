.kaiping_box {
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    z-index: 2;
}

.kaiping_image {
    width: 100%;
    height: 100%;
}

.kaiping-header {
    position: absolute;
    top: 100rpx;
    left: 5px;
    width: 96%;
    display: flex;
}


.kaiping-second,.kaiping-btn {
    font-size: 12px;
    color: purple;
    border: 1px solid purple;
    padding: 1px 6px;
    border-radius: 10px;
}
